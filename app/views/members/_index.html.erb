<% content_for :head do -%>
  <%= stylesheet_link_tag 'pagination' %>
<% end -%>

<table id="members">
  <thead>
    <tr>
      <th>Member</th>
      <th colspan="2">Actions</th>
    </tr>
  </thead>
  <%= render :partial => "members/member", :collection => members %>
</table>

<%= will_paginate members, :class => "apple_pagination" %>

<p><%= link_to "New Member", new_member_path, :class => "button" %></p>
