source 'http://rubygems.org'

gem 'rails', '3.2.13'

group :development, :test do
  gem 'debugger'
  gem 'sqlite3', '~> 1.3.6'
  gem 'timecop'
  gem 'unicorn'
end

group :production do
  gem 'mysql2', '~> 0.3.10'
  gem 'therubyracer', '>=0.11.1'
  gem 'libv8'
end

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'uglifier'
end

gem 'authlogic'
gem 'cancan'
gem 'decent_exposure', '<2.0'
